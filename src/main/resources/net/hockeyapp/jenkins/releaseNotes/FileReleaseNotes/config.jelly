<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core"
         xmlns:f="/lib/form">
    <f:entry title="${%File Name}" field="fileName">
        <f:textbox checkUrl="'descriptorByName/net.hockeyapp.jenkins.releaseNotes.FileReleaseNotes/checkFileName?value='+escape(this.value)"/>
    </f:entry>
    <f:entry title="${%Interpret Release Notes as Markdown}" field="isMarkdown">
        <f:checkbox/>
    </f:entry>
</j:jelly>
